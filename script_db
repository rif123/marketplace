) ENGINE=InnoDB DEFAULT CHARSET=latin1;

-- ----------------------------
--  Table structure for `dk_client`
-- ----------------------------
DROP TABLE IF EXISTS `dk_client`;
CREATE TABLE `dk_client` (
  `id_client` int(11) NOT NULL AUTO_INCREMENT,
  `name_client` varchar(100) NOT NULL,
  `hp_client` varchar(100) NOT NULL,
  `gender_client` enum('Pria','Wanita') NOT NULL DEFAULT 'Pria',
  `email_client` varchar(100) NOT NULL,
  `password_client` varchar(200) NOT NULL,
  `newslatter_client` enum('true','false') DEFAULT 'false',
  `creator` int(11) DEFAULT NULL,
  `created` datetime DEFAULT NULL,
  `editor` int(11) DEFAULT NULL,
  `edited` datetime DEFAULT NULL,
  PRIMARY KEY (`id_client`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=latin1;

