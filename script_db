DROP TABLE IF EXISTS `dk_bankCleint`;
CREATE TABLE `dk_bankCleint` (
  `id_bankClient` int(11) NOT NULL AUTO_INCREMENT,
  `id_client` int(11) DEFAULT NULL,
  `id_bank` int(11) DEFAULT NULL,
  `substation_bankClient` varchar(100) DEFAULT NULL,
  `name_rek_bankClient` varchar(100) DEFAULT NULL,
  `no_rek_bankClient` int(100) DEFAULT NULL,
  `fileBookrek_bankClient` varchar(100) DEFAULT NULL,
  `creator` int(11) DEFAULT NULL,
  `created` datetime DEFAULT NULL,
  `editor` int(11) DEFAULT NULL,
  `edited` datetime DEFAULT NULL,
